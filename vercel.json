{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "^/(?!api).*$",
        "dest": "/client/build/index.html"
      }
    ]
  }
  